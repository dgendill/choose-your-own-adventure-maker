    "lodash": "^4.17.4",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.6",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "http-server": "^0.10.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.4",
